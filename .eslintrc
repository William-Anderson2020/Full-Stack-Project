{
    "env": {
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    },
    "overrides": [
        {
            "files":["board.js"],
            "rules": {
                "semi": "off",
                "quotes": "off"
            }
        }
    ]
}